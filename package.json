{
	"name": "svelte-portfolio",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"devv": "vavite-loader vite dev",
		"format": "prettier --plugin-search-dir . --write .",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"preview": "vite preview",
		"sso": "aws sso login --sso-session=mugi",
		"sst:deploy": "sst deploy --stage dev",
		"sst:dev": "sst dev --stage dev",
		"test": "sst bind vitest run"
	},
	"devDependencies": {
		"@aws-sdk/client-sesv2": "^3.922.0",
		"@aws-sdk/types": "^3.922.0",
		"@eslint/compat": "^1.4.0",
		"@eslint/css": "^0.14.1",
		"@eslint/js": "^9.39.1",
		"@eslint/json": "^0.14.0",
		"@iconify/json": "^2.2.402",
		"@skeletonlabs/skeleton": "^4.2.2",
		"@skeletonlabs/skeleton-svelte": "^4.2.2",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/enhanced-img": "^0.8.4",
		"@sveltejs/kit": "^2.48.4",
		"@tailwindcss/forms": "^0.5.10",
		"@types/aws-lambda": "^8.10.157",
		"@types/node": "^24.9.2",
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.2",
		"@vavite/node-loader": "^5.1.0",
		"autoprefixer": "^10.4.21",
		"aws-cdk-lib": "2.221.0",
		"constructs": "10.4.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"prettier-plugin-svelte": "^3.4.0",
		"sst": "^3.17.21",
		"svelte": "^5.43.2",
		"svelte-check": "^4.3.3",
		"svelte-kit-sst": "2.43.5",
		"svelte-loading-spinners": "^0.3.6",
		"svelte-turnstile": "^0.11.0",
		"tailwindcss": "^4.1.18",
		"tslib": "^2.8.1",
		"typescript": "^5.9.0",
		"typescript-eslint": "^8.48.1",
		"unplugin-auto-import": "^20.2.0",
		"unplugin-icons": "^22.5.0",
		"vite": "^6.1.0"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.7.4",
		"@fontsource-variable/montserrat": "^5.2.8",
		"@fontsource/montserrat": "^5.2.8",
		"@lucide/svelte": "^0.562.0",
		"@tailwindcss/vite": "^4.1.18",
		"highlight.js": "^11.9.0",
		"sst": "3.17.25",
		"svelte-french-toast": "^2.0.0-alpha.0",
		"svelte-inview": "^4.0.4",
		"svelte-kit-sst": "2.43.5"
	}
